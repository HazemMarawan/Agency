                            </div>
                        </div>
                        <div class="w-chart">
                            <div id="daily-reservations"></div>
                        </div>
                    </div>
                </div>
            </div>


            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12 layout-spacing">
                <div class="widget widget-table-three">

                    <div class="widget-heading">
                        <h5 class="">Top Selling Hotels</h5>
                    </div>

                    <div class="widget-content">
                        <div class="table-responsive">
                            <table class="table table-scroll">
                                <thead>
                                    <tr>
                                        <th><div class="th-content">Hotel</div></th>
                                        <th><div class="th-content">Total Nights</div></th>
                                        <th><div class="th-content">City</div></th>
                                        <th><div class="th-content">Rate</div></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach(var hotel in Model.topSellingHotels) {
                                    <tr>

                                        <td>
                                            <div class="td-content product-name">
                                                @if (hotel.path != null && hotel.path != "")
                                                {
                                                    <img src="@hotel.path" alt="product">

                                                }
                                                else
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/90x90.jpg" alt="product">

                                                }
                                                <div class="align-self-center">
                                                    <p class="prd-name">@hotel.name</p>
                                                </div>
                                            </div>
                                        </td>
                                        <td><div class="td-content">@hotel.total_nights</div></td>
                                        <td><div class="td-content">@hotel.cityName</div></td>
                                        <td>
                                            <div >
                                                @if (hotel.rating == 0 || hotel.rating == null)
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/Stars/0.png" width="90" alt="Rate">
                                                }
                                                else if (hotel.rating == 1)
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/Stars/1.png" width="20" alt="Rate">
                                                }
                                                else if (hotel.rating == 1.5)
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/Stars/1.5.png" width="35" alt="Rate">
                                                }
                                                else if (hotel.rating == 2)
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/Stars/2.png" width="35" alt="Rate">
                                                }
                                                else if (hotel.rating == 2.5)
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/Stars/2.5.png" width="55" alt="Rate">
                                                }
                                                else if (hotel.rating == 3)
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/Stars/3.png" width="55" alt="Rate">
                                                }
                                                else if (hotel.rating == 3.5)
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/Stars/3.5.png" width="70" alt="Rate">
                                                }
                                                else if (hotel.rating == 4)
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/Stars/4.png" width="70" alt="Rate">
                                                }
                                                else if (hotel.rating == 4.5)
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/Stars/4.5.png" width="90" alt="Rate">
                                                }
                                                else
                                                {
                                                    <img src="~/Content/AdminPanelAssets/assets/img/Stars/5.png" width="90" alt="Rate">
                                                }

                                             
                                            </div>
                                        </td>
                                    </tr>
                                    }
                                    
                                  

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-9 col-lg-9 col-md-9 col-sm-12 col-12 layout-spacing">

