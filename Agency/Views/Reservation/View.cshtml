@model Agency.ViewModel.ReservationViewModel


<div class="modal fade" id="edit_reservation_modal" tabindex="-1" role="dialog" aria-hidden="true">
    <form action="" method="post" enctype='multipart/form-data'>
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="modal_title"></h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="form-body">
                        <div class="form-row">
                            <input type="hidden" name="id" id="id" value="@Model.id" />
                            <input type="hidden" name="company_id" id="company_id" value="@Model.company_id" />

                            <div class="form-group col-md-6">
                                <label>Company Name</label>
                                <input name="company_name" id="company_name" type="text" class="form-control radius-30" value="@Model.company_name" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Name</label>
                                <input name="reservations_officer_name" id="reservations_officer_name" type="text" value="@Model.reservations_officer_name" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Phone</label>
                                <input name="reservations_officer_phone" id="reservations_officer_phone" type="text" value="@Model.reservations_officer_phone" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Email</label>
                                <input name="reservations_officer_email" id="reservations_officer_email" type="email" value="@Model.reservations_officer_email" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Vendor</label>
                                @Html.DropDownListFor(m => m.vendor_id, new SelectList(ViewBag.Vendors, "id", "NamePlusCode"), "Select Vendor", new { @class = "form-control", @name = "vendor_id", @id = "vendor_id" })
                            </div>
                            <div class="form-group col-md-6">
                                <label>Advance Reservation Percentage</label>
                                <input name="advance_reservation_percentage" id="advance_reservation_percentage" value="@Model.advance_reservation_percentage" type="number" min="0" step="0.1" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Single Price</label>
                                <input name="single_price" id="single_price" type="number" min="0" step="0.1" value="@Model.single_price" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Vendor Single Price</label>
                                <input name="vendor_single_price" id="vendor_single_price" type="number" min="0" step="0.1" value="@Model.vendor_single_price" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Double Price</label>
                                <input name="double_price" id="double_price" type="number" min="0" step="0.1" value="@Model.double_price" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Vendor Double Price</label>
                                <input name="vendor_douple_price" id="vendor_douple_price" type="number" min="0" step="0.1" value="@Model.vendor_douple_price" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Triple Price</label>
                                <input name="triple_price" id="triple_price" type="number" min="0" step="0.1" value="@Model.triple_price" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Vendor Triple Price</label>
                                <input name="vendor_triple_price" id="vendor_triple_price" type="number" min="0" step="0.1" value="@Model.vendor_douple_price" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Quad Price</label>
                                <input name="quad_price" id="quad_price" type="number" min="0" step="0.1" value="@Model.quad_price" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Vendor Quad Price</label>
                                <input name="vendor_quad_price" id="vendor_quad_price" type="number" min="0" step="0.1" value="@Model.vendor_quad_price" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Tax</label>
                                <input name="tax" id="tax" type="number" min="0" step="0.1" value="@Model.tax" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Opener</label>
                                <select class="form-control" name="opener" id="opener">
                                    <option value="">Select Opener</option>
                                    @foreach (var open in ViewBag.Opener)
                                    {
                                        if (open.id == Model.opener)
                                        {
                                            <option value="@open.id" selected>@open.full_name</option>
                                        }
                                        else
                                        {
                                            <option value="@open.id">@open.full_name</option>
                                        }
                                    }
                                </select>
                                @*@Html.DropDownListFor(m => m.opener, new SelectList(ViewBag.Opener, "id", "full_name"), "Select Opener", new { @class = "form-control", @name = "opener", @id = "opener" })*@
                            </div>
                            <div class="form-group col-md-6">
                                <label>Closer</label>
                                <select class="form-control" name="closer" id="closer">
                                    <option value="">Select Closer</option>
                                    @foreach (var closer in ViewBag.Closer)
                                    {
                                        if (closer.id == Model.closer)
                                        {
                                            <option value="@closer.id" selected>@closer.full_name</option>
                                        }
                                        else
                                        {
                                            <option value="@closer.id">@closer.full_name</option>
                                        }
                                    }
                                </select>
                                @*@Html.DropDownListFor(m => m.closer, new SelectList(ViewBag.Closer, "id", "full_name"), "Select Closer", new { @class = "form-control", @name = "closer", @id = "closer" })*@
                            </div>
                            <div class="form-group col-md-6">
                                <label>Shift</label>
                                <select class="form-control" name="shift" id="shift">
                                    <option value="">Select shift</option>
                                    @foreach (var shift in ViewBag.Shift)
                                    {
                                        if (shift.id == Model.shift)
                                        {
                                            <option value="@shift.id" selected>@shift.shift_name</option>
                                        }
                                        else
                                        {
                                            <option value="@shift.id">@shift.shift_name</option>
                                        }
                                    }
                                </select>
                                @*@Html.DropDownListFor(m => m.shift, new SelectList(ViewBag.Shift, "id", "shift_name"), "Select Shift", new { @class = "form-control", @name = "shift", @id = "shift" })*@
                            </div>
                            <div class="form-group col-md-6">
                                <label>Currency</label>
                                <select class="form-control" name="currency" id="currency">
                                    <option value="">Select Currency</option>
                                    @foreach (var currency in ViewBag.currency)
                                    {
                                        if (currency.id == Model.currency)
                                        {
                                            <option value="@currency.id" selected>@currency.string_currency</option>
                                        }
                                        else
                                        {
                                            <option value="@currency.id">@currency.string_currency</option>
                                        }
                                    }
                                </select>
                                @*@Html.DropDownListFor(m => m.currency, new SelectList(ViewBag.currency, "id", "name"), "Select Currency", new { @class = "form-control", @name = "currency", @id = "currency" })*@
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </form>
</div>

